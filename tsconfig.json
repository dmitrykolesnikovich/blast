{
    "compilerOptions": {
        "target": "ES6",
        "module": "commonjs",
        "esModuleInterop": true,
        "forceConsistentCasingInFileNames": true,
        "strict": true,
        "strictPropertyInitialization": false,
        "skipLibCheck": true,
        "downlevelIteration": true,
        "resolveJsonModule": true,
        "sourceMap": true,
    },
    "exclude": ["node_modules"]
}
